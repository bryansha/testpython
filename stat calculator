myList = []

userInput = input("Put your numbers with space between each number: ")
print(userInput)
revisedInput = userInput.split()
revisedInput.sort()
print(revisedInput)
revisedInput = list(map(int, revisedInput))
print(revisedInput)
myList.append(revisedInput)
myList.sort()






def doing():
    mtf = input("What would you like to do now? ")
    if mtf == "add":
        addMore = input('add more then: ')
        revisedAdded = addMore.split()
        myList.append(revisedAdded)
        myList.sort()
        print(myList)

    elif mtf == "delete":
        deleteFun = input("what would you like to delete? ")
        myList.pop(deleteFun)
    elif mtf == "search":
        search = input("search for that keyword: ")
        searchResult = myList.index(search)

doing()
doing()